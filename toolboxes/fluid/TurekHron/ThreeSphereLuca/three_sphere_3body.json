{
    "Name": "Rama-Ahmadi swimmer",
    "ShortName":"RA swimmer",
    "Models":
    {
	    "equations":"Stokes"
    },
    "Parameters":
    {
	"ubar":"0",
        "dx":"0",
        "dy":"0",
        "dz":"0"
    },
    "Materials":
    {
	"Fluid":{
	    "markers":["Fluid"],
	    "physics":"fluid",
	    "rho":"1e-6", //Reynolds number  All the mesh units are in micrometers; as a result I put these constants in micrometers too
	    "mu":"1"     // I use also micrograms -> density [micrograms/(micrometer)^3; mu [micrograms/(micrometers*second)]
	},
	"Solid1":{
	    "markers":["Body"],
	    "physics":"body",
	    "rho":"1e-3"
	}
    },
    "BoundaryConditions":
    {
	    "velocity":
	    {
	        "Dirichlet":
	        {
                "BoxWalls":
                {
                    "expr":"{0,0,0}"
		            //"alemesh_bc":"fixed"
		        }
            }
	    },
	    "fluid":
	    {
	        "body":
	        {
		        "SphereCenter":
		        {
		            "markers":["SphereCenter"],
		            //"materials":"MySolid",
		            "materials":
		            {
			            "names":["Solid1"]
			            //"markers":["beam"] // optinal
		            }/*,
					"translational-velocity":"{0,0,2}",
					"angular-velocity":"{0,0,0}"*/
				},
				"SphereRight":
		        {
		            "markers":["SphereRight"],
		            //"materials":"MySolid",
		            "materials":
		            {
			            "names":["Solid1"]
			            //"markers":["beam"] // optinal
		            }/*,
			        "translational-velocity":"{0,0,2}",
					"angular-velocity":"{0,0,0}"*/
				},
				"SphereLeft":
		        {
		            "markers":["SphereLeft"],
		            //"materials":"MySolid",
		            "materials":
		            {
			            "names":["Solid1"]
			            //"markers":["beam"] // optinal
		            }/*,
			        "translational-velocity":"{0,0,2}",
					"angular-velocity":"{0,0,0}"*/
		        }
	        }
        }
    },
    "PostProcess":
    {
	    "Exports":
	    {
	        "fields":["velocity","pressure","pid","displacement","meshale.velocity"] // the parameter bdrydisp is a customfield to be imposed on
            // the boundary of the swimmer to project the displacement from ODE 
	    },
	    "Measures":
	    {
	        "Forces":"SphereCenter"
	    }
    }
    
}

